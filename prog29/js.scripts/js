// length - tamanho do array.
var arr = [1,2,3,4,5];

console.log(arr.length);

// push - add propriedades ao array e não só number.
arr.push(6);
arr.push('Qualquer coisa');

console.log(arr);

// pop - remove elementos do fim do array.
arr.pop();

console.log(arr);

// unshift - add elementos ao início do array.
arr.unshift(0);
arr.unshift('teste');

console.log(arr);

// shift - remove elementos do array no início.
arr.shift();

console.log(arr);

// acessar o último elemento do array
console.log(arr[arr.length - 1]);

// isArray - verificar se estamos trabalhando com Array.
console.log(Array.isArray(5));

console.log(Array.isArray(arr));

